/*
 * TQMLS1088A RCW basics for SD card boot
 */
BOOT_LOC=0x15
FLASH_MODE=0x2
IIC2_BASE=2

/* set boot location pointer to OCRAM (u-boot SPL start address) */
.pbi
write 0x01e00404,0x00000000
write 0x01e00400,0x1800a000
.end

#include <tcpz_nosecure_region.rcw>

/* copy u-boot SPL to OCRAM */
.pbi
blockcopy 0x40,0x00100000,0x1800a000,0x00015000
.end

